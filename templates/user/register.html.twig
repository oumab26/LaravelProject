{% block stylesheets %}
    <link rel="stylesheet" type="text/css" href="{{ asset('frontOffice/css/plugins.min.css') }}">
    <link rel="stylesheet" type="text/css"
          href="{{ asset('frontOffice/plugins/fontawesome/css/font-awesome.min.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('frontOffice/plugins/themify/themify-icons.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('frontOffice/css/style.min.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('frontOffice/css/templete.min.css') }}">
    <link class="skin" rel="stylesheet" type="text/css" href="{{ asset('frontOffice/css/skin/skin-1.min.css') }}">
    <link href="https://fonts.googleapis.com/css2?family=Lato:wght@100;300;400;700;900&amp;family=Montserrat:wght@100;200;300;400;500;600;700;800;900&amp;family=Open+Sans:wght@300;400;600;700;800&amp;family=Roboto:wght@100&amp;family=Rubik:wght@300;400;500;700;900&amp;display=swap"
          rel="stylesheet">
{% endblock %}
{% block content %}
    <script src="https://www.google.com/recaptcha/api.js" async defer></script>

    <div class="browse-job login-style3">
        <!-- Coming Soon -->
        <div class="bg-img-fix"
             style="background-image:url({{ asset('frontOffice/') }}images/background/bg6.jpg); height: 100vh;">
            <div class="row">
                <div class="col-xl-4 col-lg-5 col-md-6 col-sm-12 bg-white z-index2 relative p-a0 content-scroll skew-section left-bottom">
                    <div class="login-form style-2">
                        <div class="logo-header text-center p-tb30">
                            <a href="/"><img src="{{ asset('frontOffice/') }}images/logo.png" alt=""/></a>
                        </div>
                        <div class="clearfix"></div>
                        <div class="tab-content nav p-b30 tab">
                            <div id="forgot-password" class="tab-pane fade">
                                <form class="dez-form">
                                    <h3 class="form-title m-t0">Forget Password ?</h3>
                                    <div class="dez-separator-outer m-b5">
                                        <div class="dez-separator bg-primary style-liner"></div>
                                    </div>
                                    <p>Enter your e-mail address below to reset your password. </p>
                                    <div class="form-group">
                                        <input name="dzName" required="" class="form-control"
                                               placeholder="Email Address" type="text"/>
                                    </div>
                                    <div class="form-group clearfix text-left">
                                        <a class="site-button outline gray" href="{{ path('security_login') }}">Back</a>
                                        <button class="site-button pull-right">Submit</button>
                                    </div>
                                </form>
                            </div>
                            <div id="registration-form" class="tab-pane active">
                                {{ form_start(form) }}
                                {{ form_errors(form) }}

                                <h3 class="form-title m-t0">{{ 'user.sign-up'|trans }}</h3>
                                <div class="dez-separator-outer m-b5">
                                    <div class="dez-separator bg-primary style-liner"></div>
                                </div>
                                <p>Enter your personal details below: </p>
                                <div class="form-group">
                                    {{ form_row(form.firstName) }}
                                </div>
                                <div class="form-group">
                                    {{ form_row(form.lastName) }}
                                </div>
                                <div class="form-group">
                                    {{ form_row(form.email) }}
                                </div>
                                <div class="form-group">
                                    {{ form_row(form.password) }}
                                </div>

                                <div class="form-group">
                                    {{ form_row(form.dateOfBirth) }}
                                </div>
                                <div class="form-group">
                                    {{ form_row(form.phone) }}
                                </div>
                                <div class="g-recaptcha" data-sitekey="6Lc5CVgaAAAAAJL8V2UR9DCU3k8yUT0PNgSkmJbm"></div>
                                <br>

                                <div class="m-b30">
										<span class="custom-control custom-checkbox float-left m-r5">
											<input type="checkbox" class="custom-control-input" id="check2"
                                                   name="example1">
											<label class="custom-control-label" for="check2">I agree to the</label>
										</span>
                                    <label><a href="#">Terms of Service </a>& <a href="#">Privacy Policy</a></label>
                                </div>

                                <div class="form-group clearfix text-left">
                                    <a class="site-button outline gray"
                                       href="{{ path('security_login') }}">Back</a>
                                    <button class="site-button pull-right" formnovalidate
                                            type="submit">{{ 'user.sign-up'|trans }}</button>
                                </div>
                                <br>
                                {{ form_end(form) }}
                            </div>
                        </div>
                        <div class="bottom-footer clearfix m-t10 m-b20 row text-center">
                            <div class="col-lg-12 text-center">
								<span> Â© Copyright 2020  by <i class="fa fa-heart m-lr5 text-red heart"></i>
                            </span></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Full Blog Page Contant -->
    </div>

    <script src="https://www.google.com/recaptcha/api.js?onload=onloadCallback&render=explicit"
            async defer>
    </script>

    <script src="{{ asset('frontOffice/') }}js/combining.js"></script><!-- COMBINING JS  -->
    <script src="{{ asset('frontOffice/') }}plugins/scroll/scrollbar.min.js"></script><!-- PARTICLES  -->
    <script>
        jQuery(document).ready(function () {
            jQuery('.winHeight').css('height', $(window).height());
        });
    </script>


{% endblock %}

